<%- include('../partials/header') %>
<br><br><br>
<div>Before adding, please ensure that the Seat is not in the following list:</div>
<section id="badge-container">
  <% tickets.forEach(function(t) { %>
    <article class="badge"><%= t.name %></article>
  <% }) %>
</section>

<form id="add-ticket-form" action="/tickets" method="POST"><br><br><br><br>
  <label>Seat:</label>
  <input name="seat" required pattern="[A-F][1-9]\d?">
  <label>Price:</label>
  <input type="number" name="price">
  <input type="submit" value="Add Seat">
</form>

<%- include('../partials/footer') %>